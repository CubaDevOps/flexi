{
  "name": "cubadevops/flexi-module-logging",
  "description": "Logging module for Flexi Framework with PSR-3 support",
  "version": "1.0.0",
  "type": "flexi-module",
  "license": "MIT",
  "keywords": [
    "flexi",
    "module",
    "logging",
    "psr-3",
    "logger"
  ],
  "authors": [
    {
      "name": "Carlos Batista",
      "email": "info@carlosbatista.dev",
      "homepage": "https://carlosbatista.dev"
    }
  ],
  "require": {
    "php": ">=7.4",
    "cubadevops/flexi-contracts": "@dev"
  },
  "autoload": {
    "psr-4": {
      "CubaDevOps\\Flexi\\Modules\\Logging\\": ""
    }
  },
  "extra": {
    "flexi": {
      "module-name": "Logging",
      "module-type": "infrastructure",
      "config-files": [
        "Config/listeners.json",
        "Config/services.json"
      ],
      "provides": [
        "logging",
        "psr-3-logger",
        "event-logging"
      ]
    }
  }
}
