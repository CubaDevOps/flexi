{
  "name": "cubadevops/flexi-module-devtools",
  "description": "Development tools module for Flexi Framework",
  "version": "1.0.0",
  "type": "flexi-module",
  "license": "MIT",
  "keywords": [
    "flexi",
    "module",
    "devtools",
    "development",
    "debugging"
  ],
  "authors": [
    {
      "name": "Carlos Batista",
      "email": "info@carlosbatista.dev",
      "homepage": "https://carlosbatista.dev"
    }
  ],
  "require": {
    "php": ">=7.4",
    "cubadevops/flexi-contracts": "@dev"
  },
  "autoload": {
    "psr-4": {
      "CubaDevOps\\Flexi\\Modules\\DevTools\\": ""
    }
  },
  "extra": {
    "flexi": {
      "module-name": "DevTools",
      "module-type": "application",
      "config-files": [
        "Config/commands.json",
        "Config/queries.json"
      ],
      "provides": [
        "development-commands",
        "debugging-tools"
      ]
    }
  }
}
