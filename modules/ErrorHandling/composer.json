{
  "name": "cubadevops/flexi-module-errorhandling",
  "description": "Error handling module for Flexi Framework",
  "version": "1.0.0",
  "type": "flexi-module",
  "license": "MIT",
  "keywords": [
    "flexi",
    "module",
    "error-handling",
    "exceptions"
  ],
  "authors": [
    {
      "name": "Carlos Batista",
      "email": "info@carlosbatista.dev",
      "homepage": "https://carlosbatista.dev"
    }
  ],
  "require": {
    "php": ">=7.4",
    "cubadevops/flexi-contracts": "@dev"
  },
  "autoload": {
    "psr-4": {
      "CubaDevOps\\Flexi\\Modules\\ErrorHandling\\": ""
    }
  },
  "extra": {
    "flexi": {
      "module-name": "ErrorHandling",
      "module-type": "infrastructure",
      "config-files": [
        "Config/routes.json"
      ],
      "provides": [
        "error-handling",
        "exception-pages"
      ]
    }
  }
}
