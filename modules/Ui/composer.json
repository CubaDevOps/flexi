{
  "name": "cubadevops/flexi-module-ui",
  "description": "UI rendering module for Flexi Framework",
  "version": "1.0.0",
  "type": "flexi-module",
  "license": "MIT",
  "keywords": [
    "flexi",
    "module",
    "ui",
    "template",
    "rendering"
  ],
  "authors": [
    {
      "name": "Carlos Batista",
      "email": "info@carlosbatista.dev",
      "homepage": "https://carlosbatista.dev"
    }
  ],
  "require": {
    "php": ">=7.4",
    "cubadevops/flexi-contracts": "@dev"
  },
  "autoload": {
    "psr-4": {
      "CubaDevOps\\Flexi\\Modules\\Ui\\": ""
    }
  },
  "extra": {
    "flexi": {
      "module-name": "Ui",
      "module-type": "infrastructure",
      "config-files": [
        "Config/services.json"
      ],
      "provides": [
        "template-engine",
        "html-rendering",
        "ui-components"
      ]
    }
  }
}
