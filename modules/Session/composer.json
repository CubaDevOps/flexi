{
  "name": "cubadevops/flexi-module-session",
  "description": "Session management module for Flexi Framework",
  "version": "1.0.0",
  "type": "flexi-module",
  "license": "MIT",
  "keywords": [
    "flexi",
    "module",
    "session",
    "session-management"
  ],
  "authors": [
    {
      "name": "Carlos Batista",
      "email": "info@carlosbatista.dev",
      "homepage": "https://carlosbatista.dev"
    }
